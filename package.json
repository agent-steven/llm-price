{
  "name": "llm-price",
  "version": "1.0.0",
  "description": "Real-time LLM pricing comparison & cost calculator",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "llm-price": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "llm",
    "pricing",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "gemini",
    "cost",
    "calculator",
    "ai",
    "tokens"
  ],
  "author": "llm-price contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/llm-price"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
